# Gitignore for Next.js projects

# Dependencies
/node_modules

# Next.js build output
/.next
/out

# Production build files (if applicable, e.g., for static exports)
/dist

# Environment variables
.env
.env.local
.env.*.local

# Log files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Editor/IDE specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# OS generated files
.DS_Store
Thumbs.db

# Test coverage reports
/coverage

# Other temporary files
*.tmp
*.bak